{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "import/prefer-default-export": 0,
    "no-console": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "lines-between-class-members": 0,
    "no-useless-return": 0
  }
}
